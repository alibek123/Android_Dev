<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/orange"/>
            <corners android:radius="@dimen/ticket_time_rect_corner"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/black_opacity_38"/>
            <solid android:color="@android:color/transparent"/>
            <corners android:radius="@dimen/ticket_time_rect_corner"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/black_opacity_80"/>
            <solid android:color="@android:color/transparent"/>
            <corners android:radius="@dimen/ticket_time_rect_corner"/>
        </shape>
    </item>
</selector>
